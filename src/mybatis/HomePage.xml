    <delete id="deleteImageManagement" parameterType="int">
        delete from tb_image_management where
        <foreach item="chk_value" collection="array" open="id in (" separator="," close=")">
            #{chk_value}
        </foreach>
    </delete>

    <!--添加图片管理信息-->
    <insert id="insertImageManagement" parameterType="ImageManagement">
        <selectKey resultType="int" keyProperty="id" order="AFTER">SELECT LAST_INSERT_ID()</selectKey>
