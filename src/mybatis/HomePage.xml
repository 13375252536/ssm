            update tb_qrcode_management set qrCodeURL = #{qrCodeURL} where category = #{category}
        </update>
    <!--得到数据库中的二维码路径-->
    <select id="selectURLByWeiBo" resultType="java.lang.String">
            select qrCodeURL from tb_qrcode_management where category = "微博"
        </select>
    <select id="listPageImageManagement" parameterType="ImageManagement" resultType="ImageManagement">
        select * from tb_image_management
        <where>
            <if test="name!=null">
                and name like "%"#{name}"%"
            </if>
        </where>
        order by placedTop desc, sorting asc
    </select>
    <!--图片管理中的查询-->
